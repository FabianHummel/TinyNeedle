<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>TinyNeedle</RootNamespace>
        <Authors>Fabian Hummel</Authors>
        <Description>Super minimalistic but very capable Auto-DI framework for dotNET applications.</Description>
